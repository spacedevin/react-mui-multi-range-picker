{
  "name": "@spacedevin/react-mui-multi-range-picker",
  "version": "0.2.0",
  "description": "Multi-range date picker component for Material-UI with drag-to-select functionality",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "lib",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir dist/esm && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir dist/cjs",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test:release": "npm run build && npm publish --dry-run",
    "demo:sync": "mkdir -p examples/demo/src/lib && cp -r lib/* examples/demo/src/lib/",
    "dev": "npm run demo:sync && cd examples/demo && npm start",
    "demo:install": "cd examples/demo && npm install",
    "demo:build": "npm run demo:sync && cd examples/demo && npm run build"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/material": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@mui/x-date-pickers": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "date-fns": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.14.1",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "date-fns": "^4.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "react",
    "material-ui",
    "mui",
    "date-picker",
    "date-range",
    "multi-range",
    "calendar",
    "drag-select"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/spacedevin/react-mui-multi-range-picker.git",
    "directory": "packages/MuiMultiDateRangePicker"
  },
  "license": "PIF",
  "author": "Space"
}
